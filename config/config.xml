<?xml version="1.0" encoding="ISO-8859-1"?>
<plugin-config>
	<depends plugin-name="com.iver.cit.gvsig"/>
	<depends plugin-name="es.udc.cartolab.gvsig.users"/>
	<depends plugin-name="es.udc.cartolab.gvsig.elle"/>
	<depends plugin-name="es.udc.cartolab.gvsig.navtable"/>
	<libraries library-dir="lib"/>
	<resourceBundle name="text"/>
	<extensions>
		
		<extension class-name="es.icarto.gvsig.viasobras.CatalogExtension" description="FORMS_PANEL" active="true">
			<menu text="Vías y Obras/Catálogo"
				icon="viasobras-catalogo"
				position="30000"/>
			<tool-bar name="ViasObrasLugo" position="70">
				<action-tool icon="viasobras-catalogo" tooltip="Catálogo"
					action-command="Catalog" position="1"/>
			</tool-bar>
		</extension>

		<extension class-name="es.icarto.gvsig.viasobras.FormCarreterasExtension" description="FORMS_PANEL" active="true">
			<menu action-command="separator" text="Vías y Obras" position="30099" is_separator="true"/>
			<menu text="Vías y Obras/Carreteras"
				icon="viasobras-carreteras"
				position="30100"/>
			<tool-bar name="ViasObrasLugo" position="70">
				<action-tool icon="viasobras-carreteras" tooltip="Carreteras" action-command="FormCarreteras" position="2"/>
			</tool-bar>
		</extension>

		<extension class-name="es.icarto.gvsig.viasobras.FormVariantesExtension" description="FORMS_PANEL" active="true">
			<menu text="Vías y Obras/Tramos antiguos"
				icon="viasobras-variantes"
				position="30200"/>
			<tool-bar name="ViasObrasLugo" position="70">
				<action-tool icon="viasobras-variantes" tooltip="Tramos antiguos" action-command="FormVariantes" position="3"/>
			</tool-bar>
		</extension>
		
		<extension class-name="es.icarto.gvsig.viasobras.FormRampasExtension" description="FORMS_PANEL" active="true">
			<menu text="Vías y Obras/Rampas"
				icon="viasobras-rampas"
				position="30300"/>
			<tool-bar name="ViasObrasLugo" position="70">
				<action-tool icon="viasobras-rampas" tooltip="Rampas" action-command="FormRampas" position="4"/>
			</tool-bar>
		</extension>

		<extension class-name="es.icarto.gvsig.viasobras.RecalculateCaracteristicasExtension" description="FORMS_PANEL" active="true">
			<menu text="Vías y Obras/Recalcular características"
				icon="viasobras-recalcular-caracteristicas"
				position="30400"/>
		</extension>

		<extension class-name="es.icarto.gvsig.viasobras.FormActuacionAltaExtension" description="FORMS_PANEL" active="true">
			<menu action-command="separator" text="Vías y Obras" position="30499" is_separator="true"/>
			<menu text="Vías y Obras/Alta actuación"
				icon="viasobras-actuacion-alta"
				position="30500"/>
			<tool-bar name="ViasObrasLugo" position="70">
				<action-tool icon="viasobras-actuacion-alta" tooltip="Alta actuación" action-command="FormActuacionAlta" position="5"/>
			</tool-bar>
		</extension>		

		<extension class-name="es.icarto.gvsig.viasobras.FormActuacionesExtension" description="FORMS_PANEL" active="true">
			<menu text="Vías y Obras/Actuaciones"
				icon="viasobras-actuaciones"
				position="30600"/>
			<tool-bar name="ViasObrasLugo" position="70">
				<action-tool icon="viasobras-actuaciones" tooltip="Actuaciones" action-command="FormActuaciones" position="6"/>
			</tool-bar>
		</extension>

		<extension class-name="es.icarto.gvsig.viasobras.QueriesEstadoExtension" description="FORMS_PANEL" active="true">
			<menu action-command="separator" text="Vías y Obras" position="30699" is_separator="true"/>
			<menu text="Vías y Obras/Consultas"
				icon="viasobras-consultas"
				position="30700"/>
			<tool-bar name="ViasObrasLugo" position="70">
				<action-tool icon="viasobras-consultas" tooltip="Consultas estado actual"
					action-command="QueriesEstado" position="7"/>
			</tool-bar>
		</extension>		

	</extensions>
</plugin-config>
